package comparainteiros;
import java.util.Scanner    
/**
 * Sara lym 
 * aplicação compara inteiros , um estudo de estruturas de condição com IF 
 * 
 */
public class ComparaInteiros {

    /**Cria o scaner para obter dados a partir da linha de comando 
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        // TODO code application logic here
        Scanner input = new Scanner(System.in);
int numero1 ;
int numero2 ;

System.out.print("Entre com o primeiro numero :" );
numero1 = input.nextInt(); // le o primeiro numero fornecido pelo usuario

System.out.print("Entre com o segundo numero :");
numero2 = input.nextInt(); 

if (numero1 == numero2 )
     System.out.printf("%d == %d%n",numero1, numero2 );

if (numero1 != numero2)
    System.out.printf( "%d != %d%n", numero1,numero2 ) ;

if (numero1 < numero2 )
     System.out.printf("%d  < %d%n",numero1, numero2 );

if (numero1 > numero2)
    System.out.printf( "%d > %d%n", numero1,numero2 ) ;
        
if (numero1 <= numero2 )
     System.out.printf("%d <= %d%n",numero1, numero2 );

if (numero1 >= numero2)
    System.out.printf( "%d >= %d%n", numero1,numero2 ) ;
    }
    //fim do metodo main 
}
//fim da classe comparacão inteiros 
